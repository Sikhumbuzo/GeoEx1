{
	"welcomeFile": "webapp/index.html",
	"authenticationMethod": "route",
	"websockets": {
		"enabled": true
	},
	"routes": [{
		"source": "(.*)(.xsjs)",
		"destination": "js_api",
		"csrfProtection": false,
		"authenticationType": "xsuaa"
	}, {
		"source": "^/webapp/(.*)",
		"localDir": "resources",
		"replace": {
			"pathSuffixes": [
				"index.html"
			],
			"vars": [
				"sapui5url"
			]
		}
	}]
}